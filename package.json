    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/react-measure": "^2.0.8",
    "@types/react-redux": "^7.1.23",
    "@types/react-router-dom": "^5.3.3",
    "@types/yup": "^0.29.13",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
